SQL> CREATE TABLE Stud_Marks (
        roll NUMBER PRIMARY KEY,
        name VARCHAR2(50),
        total_marks NUMBER(5)
    );

SQL> INSERT INTO Stud_Marks VALUES (1, 'Asha Patil', 1450);

SQL> INSERT INTO Stud_Marks VALUES (2, 'Rahul Deshmukh', 960);

SQL> INSERT INTO Stud_Marks VALUES (3, 'Priya Kulkarni', 870);

SQL> INSERT INTO Stud_Marks VALUES (4, 'Vikram Joshi', 750);

SQL> COMMIT;

SQL> CREATE TABLE Result (
        roll NUMBER,
        name VARCHAR2(50),
        class VARCHAR2(30)
    );

SQL> CREATE OR REPLACE PROCEDURE proc_Grade IS
         v_roll Stud_Marks.roll%TYPE;
         v_name Stud_Marks.name%TYPE;
         v_marks Stud_Marks.total_marks%TYPE;
         v_class VARCHAR2(30);
         CURSOR c1 IS
             SELECT roll, name, total_marks FROM Stud_Marks;
     BEGIN
         FOR rec IN c1 LOOP
             v_roll := rec.roll;
             v_name := rec.name;
             v_marks := rec.total_marks;
             IF v_marks >= 990 AND v_marks <= 1500 THEN
                 v_class := 'Distinction';
             ELSIF v_marks >= 900 AND v_marks <= 989 THEN
                 v_class := 'First Class';
             ELSIF v_marks >= 825 AND v_marks <= 899 THEN
                 v_class := 'Higher Second Class';
             ELSE
                 v_class := 'Fail';
             END IF;
             INSERT INTO Result VALUES (v_roll, v_name, v_class);
         END LOOP;
         DBMS_OUTPUT.PUT_LINE('Student grades categorized successfully.');
     END;
     /

SQL> SET SERVEROUTPUT ON;

SQL> BEGIN
         proc_Grade;
     END;
     /

SQL> SELECT * FROM Result;

